@{
    ViewData["Title"] = "Xuất ra file Excel";
    ViewBag.EActive = "active";
}
@model IEnumerable<Models.DS_Giai>
<hr/>
<a asp-action="DownloadExcel" asp-controller="File" class="mb-3 btn btn-success">Xuất file Excel trống</a>

<h4>Chọn giải đấu để xuất ra file Excel</h4>
@foreach (var item in Model)
{
    <div class="row ml-2 mb-1">
        <a asp-action="Export" asp-controller="File" asp-route-id="@item.Id">
            • Giải @item.Ten
        </a>
    </div>
    
}